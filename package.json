{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "## Description Building an initial website to host projects for my developer portfolio.",
  "main": "script.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass  prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosswilson2306/Portfolio-Website.git"
  },
  "author": "Ross Wilson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rosswilson2306/Portfolio-Website/issues"
  },
  "homepage": "https://github.com/rosswilson2306/Portfolio-Website#readme",
  "dependencies": {
    "node-sass": "^5.0.0"
  }
}
